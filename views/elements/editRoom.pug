div.editRoomBox(v-if="editRoom" v-on:click="editRoom=null")
    div.editRoom(v-on:click.stop.prevent=";;")
        div.flexBox
            div.half
                input.eventItemInput.eventTitle(placeholder="Напишите название события" v-model="editRoom.title" v-on:change="changeRoom()")
                div.editLine
                    span.iconPre(v-on:click="changeRoomTime(editRoom, $event)")
