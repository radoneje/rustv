                        <rect id="Icon-Rectangle-path" fill-opacity="0" x="0" y="0" width="12" height="12"></rect>
                        </g>
                        </g>
                        </g>
                        </g>
                        </g>
                        </svg>
                    span(v-on:click="changeRoomDate(editRoom, $event)") {{moment(editRoom.date).format('dddd, DD MMMM YYYY г. ')}}
                textarea.eventItemInput.eventTitle(placeholder="Напишите заголовок" v-model="editRoom.description" v-on:change="changeRoom()")
            div.half
                    div.regGroupTitle Используемые сервисы:
                        div.regGroupItems
                        div.regGroupItem(v-bind:class="{active:editRoom.isChat}" v-on:click="changeRoomSect('isChat')") Чат
                        div.regGroupItem(v-bind:class="{active:editRoom.isQ}" v-on:click="changeRoomSect('isQ')") Вопросы
                        div.regGroupItem(v-bind:class="{active:editRoom.isQpreMod}" v-on:click="changeRoomSect('isQpreMod')")
                            span Премодерация вопросов
                            span &nbsp;&nbsp;
                                include dollar
                        div.regGroupItem(v-bind:class="{active:editRoom.isPres}" v-on:click="changeRoomSect('isPres')") Показ слайдов
                            span &nbsp;&nbsp;
                                include dollar
                        div.regGroupItem(v-bind:class="{active:editRoom.isFile}" v-on:click="changeRoomSect('isFile')") Обмен файлами
                            span &nbsp;&nbsp;
                                include dollar
                        div.regGroupItem(v-bind:class="{active:editRoom.isRecordVideo}" v-on:click="changeRoomSect('isRecordVideo')") Видеовопрос спикеру
                        div.regGroupItem(v-bind:class="{active:editRoom.isLiveVideo}" v-on:click="changeRoomSect('isLiveVideo')") Видеовопрос спикеру в прямом эфире
                            span &nbsp;&nbsp;
                                include dollar
                        div.regGroupItem(v-bind:class="{active:editRoom.isRecord}" v-on:click="changeRoomSect('isRecord')") Запись сессии
                            span &nbsp;&nbsp;
                                include dollar
        div.flexBox(style="margin-top:50px")
            div.half
                div.regGroupTitle
                    span
                        include link
                    span &nbsp; Ссылка на страницу для пользователя:
                    div.paddingTop.linkLabel https://conf.rustv.ru/room/{{editRoom.id}}
                div.marginTop.regGroupTitle
                    span
                        include link
                    span &nbsp;Ссылка на страницу для модератора:
                    div.paddingTop.linkLabel https://conf.rustv.ru/moderator/{{editRoom.id}}
                div.marginTop.regGroupTitle
                    span
                        include link
                    span &nbsp; Ссылка на страницу для спикера:
                    div.paddingTop.linkLabel https://conf.rustv.ru/speaker/{{editRoom.id}}
            div.half
                div.regGroupTitle
                    span
                        include password
                    span &nbsp; Пароль спикера и модератора:
                    input.marginTop.eventItemInput.eventTitle(type="text" placeholder="Напишите пароль здесь" v-model="editRoom.password" v-on:change="changeRoom()")


