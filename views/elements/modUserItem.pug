        div.qItemMessageText {{item.messageToUser}}
        div.qItemMessageInputGroup
            input(type="text" placeholder="Сообщение пользователю" v-model="item.messageToUserText" v-on:keypress="if($event.keyCode==13) messageToUser(item)")
